{
  "name": "egg-ssr-pages",
  "version": "1.0.0",
  "description": "Create roe/egg route definitions to host server-side rendered pages by using next/nuxt",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_DEBUG=egg-ssr-pages nyc ava --timeout=10s --verbose test/*.test.js",
    "test:single": "NODE_DEBUG=egg-ssr-pages nyc ava --timeout=10m --verbose test/normal.test.js",
    "test-no-report": "NODE_DEBUG=egg-ssr-pages nyc ava --timeout=10s --verbose test/*.test.js",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/egg-ssr-pages.git"
  },
  "keywords": [
    "egg-ssr-pages"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/egg-ssr-pages/issues"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "^1.4.0",
    "codecov": "^3.2.0",
    "eslint": "^5.15.3",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.16.0",
    "fs-extra": "^7.0.1",
    "nyc": "^13.3.0",
    "roe": "^1.1.0",
    "roe-scripts": "^1.0.0",
    "supertest": "^4.0.2",
    "tmp-promise": "^1.0.5"
  },
  "dependencies": {
    "err-object": "^4.1.0",
    "lru-cache": "^5.1.1",
    "pre-suf": "^1.1.1"
  }
}
