{
  "name": "egg-ssr-pages",
  "version": "1.1.1",
  "description": "Create roe/egg route definitions to host server-side rendered pages by using next/nuxt",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_DEBUG=egg-ssr-pages nyc ava --timeout=10s --verbose",
    "test:dev": "NODE_DEBUG=egg-ssr-pages nyc ava --timeout=10s --verbose",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/egg-ssr-pages.git"
  },
  "keywords": [
    "egg-ssr-pages"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false,
    "files": [
      "test/*.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/egg-ssr-pages/issues"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.17.2",
    "fs-extra": "^8.0.1",
    "next": "^8.1.0",
    "nyc": "^14.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "roe": "^2.0.0",
    "supertest": "^4.0.2",
    "tmp-promise": "^1.1.0"
  },
  "dependencies": {
    "core-util-is": "^1.0.2",
    "err-object": "^4.3.1",
    "lru-cache": "^5.1.1",
    "pre-suf": "^1.1.1"
  }
}
